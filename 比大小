#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int main()
{
	int a,b,c,max;
    scanf("%d,%d,%d", &a, &b, &c);
    max = a;
    if (max < b)
	    max = b;
    if (max < c)
        max = c;
    printf("%d\n", max);
    return 0;
}
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>
int Max(int a, int b, int c)
{
	if (a < b & b > c)
		return b;
	if (a < b & b < c)
		return c;
	if (a > b & b > c)
		return a;
}
int main()
{
	int a = 10;
	int b = 20;
	int c = 15;
	int max = 0;
	max = Max(a, b, c);
	printf("%d\n", max);
	return 0;
}
